# 飞书应用配置
FEISHU_APP_ID=your_feishu_app_id
FEISHU_APP_SECRET=your_feishu_app_secret
FEISHU_ENCRYPT_KEY=your_feishu_encrypt_key
FEISHU_VERIFICATION_TOKEN=your_feishu_verification_token

# 飞书多维表格配置
FEISHU_BITABLE_APP_TOKEN=your_bitable_app_token
FEISHU_BITABLE_TABLE_ID=your_bitable_table_id

# n8n 工作流配置
N8N_WEBHOOK_URL=your_n8n_webhook_url
N8N_API_KEY=your_n8n_api_key
N8N_HOST=http://localhost:5678
N8N_USER=admin
N8N_PASSWORD=your_n8n_password
POSTGRES_PASSWORD=your_postgres_password

# AI 模型 API 配置
# OpenAI
OPENAI_API_KEY=your_openai_api_key
OPENAI_BASE_URL=https://api.openai.com
OPENAI_TIMEOUT=60000
OPENAI_RETRIES=3

# Stability AI
STABILITY_API_KEY=your_stability_api_key
STABILITY_BASE_URL=https://api.stability.ai
STABILITY_TIMEOUT=120000
STABILITY_RETRIES=3

# 自定义 AI 服务（兼容 OpenAI API 格式）
CUSTOM_AI_API_KEY=your_custom_api_key
CUSTOM_AI_BASE_URL=https://your-custom-ai-api.com
CUSTOM_AI_TIMEOUT=120000
CUSTOM_AI_RETRIES=3

# Runway
RUNWAY_API_KEY=your_runway_api_key
RUNWAY_BASE_URL=https://api.runwayml.com
RUNWAY_TIMEOUT=300000
RUNWAY_RETRIES=3

# Pika Labs
PIKA_API_KEY=your_pika_api_key
PIKA_BASE_URL=https://api.pika.art
PIKA_TIMEOUT=300000
PIKA_RETRIES=3

# 其他 AI 服务（通过自定义 API 集成）
MIDJOURNEY_API_KEY=your_midjourney_api_key
FLUX_API_KEY=your_flux_api_key

# 应用配置
NEXT_PUBLIC_API_URL=http://localhost:3000/api
NODE_ENV=development

# 存储配置
STORAGE_TYPE=local # local | s3 | oss
STORAGE_PATH=./uploads
AWS_ACCESS_KEY_ID=your_aws_access_key
AWS_SECRET_ACCESS_KEY=your_aws_secret_access_key
AWS_REGION=us-east-1
AWS_S3_BUCKET=your_s3_bucket_name